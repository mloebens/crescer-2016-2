<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <application>
        <resource-bundle>
            <base-name>messages</base-name>
            <var>bundle</var>
        </resource-bundle>
        <locale-config>
            <default-locale>pt_BR</default-locale>
        </locale-config>
    </application>
    <navigation-rule>
    <from-view-id>/index.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>filme</from-outcome>
        <to-view-id>/filme.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>classificacao</from-outcome>
        <to-view-id>/classificacao</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>genero</from-outcome>
        <to-view-id>/genero.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>idioma</from-outcome>
        <to-view-id>/idioma.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>elenco</from-outcome>
        <to-view-id>/elenco.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>ator</from-outcome>
        <to-view-id>/ator.xhtml</to-view-id>
    </navigation-case>
</navigation-rule>
    
    
</faces-config>
