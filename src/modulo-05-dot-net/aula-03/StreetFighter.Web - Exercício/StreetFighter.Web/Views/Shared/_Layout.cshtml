
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <title>Wiki - Street Fighter</title>
    @RenderSection("Css", false)
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <div class="menu">
            <span class="menu-item">
                <a href="~/StreetFighter/Index">Inicio</a>
            </span>
            <span class="menu-item">
                <a href="~/StreetFighter/ListaDePersonagens">Lista de Personagens</a>
            </span>
            <span class="menu-item">
                <a href="~/StreetFighter/Sobre">Sobre</a>
            </span>
        </div>

        <div class="conteudo">
            <div class="conteudo-cabecalho">
                <h1>@ViewBag.Title</h1>
            </div>
            @RenderBody()
        </div>
    </div>
    @RenderSection("Scripts", false)
</body>
</html>


